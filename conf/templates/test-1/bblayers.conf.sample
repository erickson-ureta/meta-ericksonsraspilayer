# POKY_BBLAYERS_CONF_VERSION is increased each time build/conf/bblayers.conf
# changes incompatibly
POKY_BBLAYERS_CONF_VERSION = "2"

BBPATH = "${TOPDIR}"
BBFILES ?= ""

BBLAYERS ?= " \
  ${TOPDIR}/../erickson-raspi-yocto/poky/meta \
  ${TOPDIR}/../erickson-raspi-yocto/poky/meta-poky \
  ${TOPDIR}/../erickson-raspi-yocto/poky/meta-yocto-bsp \
  ${TOPDIR}/../erickson-raspi-yocto/meta-raspberrypi \
  ${TOPDIR}/../erickson-raspi-yocto/meta-openembedded/meta-oe \
  ${TOPDIR}/../erickson-raspi-yocto/meta-openembedded/meta-multimedia \
  ${TOPDIR}/../erickson-raspi-yocto/meta-openembedded/meta-networking \
  ${TOPDIR}/../erickson-raspi-yocto/meta-openembedded/meta-python \
  ##OEROOT##/../meta-ericksonsraspilayer \
  "

